{"remainingRequest":"/Users/apple/Document/1010/vue-admin-template/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/apple/Document/1010/vue-admin-template/src/views/edu/teacher/list.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/apple/Document/1010/vue-admin-template/src/views/edu/teacher/list.vue","mtime":1632883698450},{"path":"/Users/apple/Document/1010/vue-admin-template/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/apple/Document/1010/vue-admin-template/node_modules/babel-loader/lib/index.js","mtime":315532800000},{"path":"/Users/apple/Document/1010/vue-admin-template/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/apple/Document/1010/vue-admin-template/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["list.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6GA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;;AAGA;;AAEA","file":"list.vue","sourceRoot":"src/views/edu/teacher","sourcesContent":["<template>\n  <div class=\"app-container\">\n    讲师列表\n    <!-- 查询表单 -->\n    <el-form :inline=\"true\" class=\"demo-form-inline\">\n      <el-form-item>\n        <el-input v-model=\"teacherQuery.name\" placeholder=\"讲师名\"></el-input>\n      </el-form-item>\n      <el-form-item>\n        <el-select v-model=\"teacherQuery.level\" placeholder=\"讲师头衔\">\n          <el-option :value=\"1\" label=\"高价讲师\"></el-option>\n          <el-option :value=\"2\" label=\"首席讲师\"></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"添加时间\">\n        <el-date-picker\n          v-model=\"teacherQuery.gmtCreate\"\n          type=\"datetime\"\n          placeholder=\"选择开始时间\"\n          value-format=\"yyyy-MM-dd HH:mm:ss\"\n          default-time=\"00:00:00\"\n        />\n      </el-form-item>\n      <el-form-item>\n        <el-date-picker\n          v-model=\"teacherQuery.gmtModified\"\n          type=\"datetime\"\n          placeholder=\"选择截止时间\"\n          value-format=\"yyyy-MM-dd HH:mm:ss\"\n          default-time=\"00:00:00\"\n        />\n      </el-form-item>\n     <el-form-item>\n       <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"getList()\">查询</el-button>\n       <el-button type=\"default\" @click=\"resetData()\">清空</el-button>\n     </el-form-item>\n</el-form>\n    <el-table\n      :data=\"list\"\n      border\n      fit\n      highlight-current-row>\n\n      style=\"width: 100%\">\n      <el-table-column\n        label=\"序号\"\n        align=\"center\"\n        width=\"70\">\n        <template slot-scope=\"scope\">\n          {{(page-1)*limit+scope.$index+1}}\n        </template>\n      </el-table-column>\n      <el-table-column\n        prop=\"name\"\n        label=\"名称\"\n        width=\"80\">\n      </el-table-column>\n      <el-table-column\n        label=\"头衔\"\n        width=\"80\">\n        <template slot-scope=\"scope\">\n          {{scope.row.level===1?'高级讲师':'首席讲师'}}\n        </template>\n      </el-table-column>\n      <el-table-column\n        prop=\"intro\"\n        label=\"资历\"\n        >\n      </el-table-column>\n       <el-table-column\n        prop=\"gmtCreate\"\n        label=\"添加时间\"\n        width=\"160\"\n        >\n      </el-table-column>\n      <el-table-column\n        prop=\"sort\"\n        label=\"排序\"\n        width=\"60\"\n        >\n      </el-table-column>\n      <el-table-column\n        label=\"操作\"\n        width=\"200\"\n        align=\"center\"\n        >\n        <template slot-scope=\"scope\">\n          <router-link :to=\"'/teacher/save/'+scope.row.id\">\n            <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-edit\">修改</el-button>\n          </router-link>\n            <el-button type=\"danger\" size=\"mini\" icon=\"el-icon-delete\" @click=\"removeDataById(scope.row.id)\">删除</el-button>            \n        </template>\n      </el-table-column>  \n    </el-table>\n    <!-- 分页 -->\n    <el-pagination\n      background\n      align=\"center\"\n      layout=\"prev, pager, next\"\n      :current-page=\"page\"\n      :page-size=\"limit\"\n      :total=\"total\"\n      @current-change=\"getList\"\n      >\n</el-pagination>\n  </div>\n</template>\n<script>\n\n//引入teacher.js\nimport teacher from '@/api/edu/teacher.js'\nexport default {\n  // 写核心代码位置\n  data(){ //定义变量和初始值\n    return{\n      list:null, //查询之后接口返回集合\n      page:1, //当前页\n      limit:10, //每页记录数\n      total:0, //总记录数\n      teacherQuery:{} //条件封装对象\n    }\n  },\n  created() { // 页面渲染之前执行，一般调用methods定义的方法\n    //调用\n    this.getList()\n  },\n  methods:{ // 创建具体的方法，调用teacher.js定义的方法\n    // 讲师列表的方法\n    getList(page=1){\n      this.page=page\n      teacher.getTeacherListPage(this.page,this.limit,this.teacherQuery)\n        .then(response =>{\n            //response接口返回的数据\n            // console.log(response)\n            this.list = response.data.records\n            this.total = response.data.total\n            console.log(this.list) \n            console.log(this.total)\n        })//请求成功\n        .catch(error =>{\n            console.log(error)\n        }) //请求失败\n    },\n    resetData(){ //情况的方法\n    //表单输入项数据清空\n    this.teacherQuery = {}\n    //查询所有讲师数据\n    this.getList()\n\n    },\n    removeDataById(id){\n        this.$confirm('此操作将永久删除讲师记录, 是否继续?', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'warning'\n        }).then(() => { //点击确定\n          teacher.deleteTeacherId(id)\n          .then(response =>{ //删除成功\n            // 提示信息\n            this.$message({\n            type: 'success',\n            message: '删除成功!'\n            });\n            // 刷新列表页面\n            this.getList()\n\n          })\n        })\n    }\n    \n\n  }\n\n}\n</script>"]}]}