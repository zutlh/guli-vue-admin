r keyword\" style=\"margin-bottom:30px;\" />\n\n    <el-tree\n      ref=\"tree2\"\n      :data=\"data2\"\n      :props=\"defaultProps\"\n      :filter-node-method=\"filterNode\"\n      class=\"filter-tree\"\n      default-expand-all\n    />\n\n  </div>\n</template>\n\n<script>\nimport subject from '@/api/edu/subject'\nexport default {\n\n  data() {\n    return {\n      filterText: '',\n      data2: [],      //返回所有分类数据\n      defaultProps: {\n        children: 'children',\n        label: 'title'\n      }\n    }\n  },\n  watch: {\n    filterText(val) {\n      this.$refs.tree2.filter(val)\n    }\n  },\n  created(){\n    this.getAllSubjectList()\n  },\n  methods: {\n    filterNode(value, data) {\n      if (!value) return true\n      return data.title.toLowerCase().indexOf(value.toLowerCase()) !== -1\n    },\n    getAllSubjectList(){\n      subject.getSubjectList()\n        .then(response =>{\n          this.data2 = response.data.list\n        })\n    }\n  }\n}\n</script>\n\n"],"sourceRoot":"src/views/edu/subject"}]}