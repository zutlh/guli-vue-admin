label=\"讲师名称\">\n        <el-input v-model=\"teacher.name\"/>\n      </el-form-item>\n      <el-form-item label=\"讲师排序\">\n        <el-input-number v-model=\"teacher.sort\" controls-position=\"right\" min=\"0\"/>\n      </el-form-item>\n      <el-form-item label=\"讲师头衔\">\n        <el-select v-model=\"teacher.level\" clearable placeholder=\"请选择\">\n          <!-- 数据类型一定要和取出的json中的一致，否则没法回填\n               因此，这里value使用动态绑定的值,保证其数据类型是number\n           -->\n          <el-option label=\"高级讲师\" :value=\"1\"></el-option>\n          <el-option label=\"首席讲师\" :value=\"2\"></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"讲师资历\">\n        <el-input v-model=\"teacher.carrer\"/>\n      </el-form-item>\n      <el-form-item label=\"讲师简介\">\n        <el-input v-model=\"teacher.info\" :rows=\"10\" type=\"textarea\"/>\n      </el-form-item>\n      <!-- 讲师头像:todo -->\n      <el-form-item>\n        <el-button type=\"primary\" :disabled=\"saveBtnDisabled\" @click=\"saveOrUpdate\">保存</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport teacherApi from '@/api/edu/teacher.js'\n\nexport default {\n  data(){\n    return{\n      teacher:{\n        name:'',\n        sort: 0,\n        level: 1,\n        career:'',\n        intro:'',\n        avatar:'' \n      },\n      saveBtnDisabled:false // 保存按钮是否禁用\n    }\n  },\n  created(){\n\n  },\n  methods:{\n    saveOrUpdate(){\n      // 添加\n      this.saveTeacher()\n\n    },\n    // 添加讲师的方法\n    saveTeacher(){\n      teacherApi.addTeacher(this.teacher)\n        .then(response =>{ //添加成功\n          //提示信息\n          this.$message({\n            type: 'success',\n            message: '添加成功！'\n          });\n          //回到列表页面\n          this.$router.push({path:'/teacher/table'})\n        })\n    }\n  }\n}\n</script>"],"sourceRoot":"src/views/edu/teacher"}]}